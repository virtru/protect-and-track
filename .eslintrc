{
  "extends": [
    "eslint:recommended",
    "plugin:vitest-globals/recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "vitest-globals/env": true
  },
  "overrides": [
    {
      "files": ["*.jsx", "*.js"]
    }
  ],
 "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": "off"
  }
}
