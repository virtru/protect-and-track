{
  "name": "protect-and-track",
  "version": "1.2.5",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "classnames": "^2.3.1",
    "client-oauth2": "^4.3.3",
    "clipboard": "^2.0.8",
    "dropbox": "^4.0.28",
    "json2csv": "^4.5.4",
    "localforage": "^1.9.0",
    "moment": "^2.29.1",
    "react": "^16.14.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "redux-zero": "^5.1.7",
    "virtru-sdk-current": "npm:virtru-sdk@2.3.2",
    "virtru-sdk-lts": "npm:virtru-sdk@2.1.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtru/protect-and-track.git"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "HTTPS=true PORT=443 HOST='local.virtru.com' react-scripts start",
    "build": "react-scripts build",
    "license": "./license-update",
    "pretty": "prettier --write \"./**/*.{css,md,js,jsx,json}\"",
    "test": "eslint src && react-scripts test --watchAll=false",
    "test-watch": "react-scripts test",
    "test-e2e": ".buildkite/scripts/e2e.sh",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^8.0.9",
    "husky": "^2.7.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "react-scripts": "^3.4.4",
    "react-test-renderer": "^16.14.0"
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index -g",
      "pre-commit": "pretty-quick --staged && .buildkite/scripts/shfmt"
    }
  }
}
