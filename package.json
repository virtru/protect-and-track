{
  "name": "protect-and-track",
  "version": "1.3.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@datadog/browser-logs": "^4.47.0",
    "@datadog/browser-rum": "^4.47.0",
    "@json2csv/plainjs": "^7.0.2",
    "@virtru-private/oidc-client-js": "^4.0.0",
    "classnames": "^2.3.2",
    "localforage": "^1.10.0",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "rc-scrollbars": "^1.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux-zero": "^5.1.7",
    "virtru-sdk": "^5.1.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtru/protect-and-track.git"
  },
  "scripts": {
    "start": "HTTPS=true PORT=443 HOST='local.virtru.com' vite start",
    "ci-e2e-test": "npm run start & ./wait-for-it.sh local.virtru.com:443 -t 30000 -- npm run playwright-test",
    "build": "vite build",
    "license": "./license-update",
    "pretty": "prettier --write \"./**/*.{css,md,js,jsx,json,yml}\"",
    "test": "eslint && vite build && vitest",
    "playwright-test": "playwright test",
    "playwright-debug": "playwright test --debug",
    "show-report": "playwright show-report",
    "test-e2e": ".buildkite/scripts/e2e.sh"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@opentdf/client": "^1.3.0",
    "@playwright/test": "^1.37.1",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/ui": "^0.34.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "husky": "^8.0.3",
    "prettier": "3.0.2",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.1.6",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.34.2"
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index -g",
      "pre-commit": "pretty-quick --staged && .buildkite/scripts/shfmt"
    }
  }
}
