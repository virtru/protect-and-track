{
  "name": "sdk-share",
  "version": "1.2.2",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "classnames": "^2.2.6",
    "client-oauth2": "^4.2.5",
    "clipboard": "^2.0.4",
    "dropbox": "^4.0.28",
    "json2csv": "^4.5.2",
    "localforage": "^1.7.3",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.3.1",
    "redux-zero": "^5.0.4",
    "virtru-sdk-current": "npm:virtru-sdk@^2.0.0",
    "virtru-sdk-lts": "npm:virtru-sdk@^1.6.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtru/protect-and-track.git"
  },
  "scripts": {
    "start": "HTTPS=true PORT=443 HOST='local.virtru.com' react-scripts start",
    "build": "react-scripts build",
    "license": "./license-update",
    "pretty": "prettier --write \"./**/*.{css,md,js,jsx,json}\"",
    "audit": "audit-ci --config .audit-ci.json",
    "test": "eslint src && react-scripts test --watchAll=false",
    "test-watch": "react-scripts test",
    "test-e2e": ".buildkite/scripts/e2e.sh",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.7",
    "audit-ci": "^2.3.0",
    "husky": "^2.7.0",
    "jest-dom": "^3.5.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "react-test-renderer": "^16.8.6"
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index -g",
      "pre-commit": "pretty-quick --staged && ./license-update --staged && .buildkite/scripts/shfmt"
    }
  }
}
