{
  "name": "protect-and-track",
  "version": "1.2.8",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@virtru/oidc-client-js": "^1.0.6",
    "classnames": "^2.3.2",
    "json2csv": "^5.0.7",
    "localforage": "^1.10.0",
    "moment": "^2.29.4",
    "rc-scrollbars": "^1.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "redux-zero": "^5.1.7",
    "virtru-sdk": "npm:virtru-sdk@3.0.4-beta.7.10"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtru/protect-and-track.git"
  },
  "scripts": {
    "start": "sudo HTTPS=true PORT=443 HOST='local.virtru.com' react-scripts start",
    "build": "react-scripts build",
    "license": "./license-update",
    "pretty": "prettier --write \"./**/*.{css,md,js,jsx,json,yml}\"",
    "test": "eslint src && react-scripts test --watchAll=false",
    "test-watch": "react-scripts test",
    "test-e2e": ".buildkite/scripts/e2e.sh",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "husky": "^8.0.1",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0"
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index -g",
      "pre-commit": "pretty-quick --staged && .buildkite/scripts/shfmt"
    }
  }
}
